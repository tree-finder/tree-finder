{
    "name": "@tree-finder/base",
    "version": "1.0.3",
    "description": "browser/tree widget implemented as a custom DOM element. Built on top of regular-table",
    "license": "BSD-3-Clause",
    "author": "Max Klein <telamonian@users.noreply.github.com>",
    "homepage": "https://github.com/tree-finder/tree-finder",
    "bugs": {
        "url": "https://github.com/tree-finder/tree-finder/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/tree-finder/tree-finder.git"
    },
    "main": "dist/tree-finder.js",
    "style": "dist/tree-finder.css",
    "types": "types/index.d.ts",
    "files": [
        "dist/**/*",
        "src/**/*",
        "style/**/*.{css,eot,gif,html,jpg,json,less,png,scss,svg,ttf,woff2}",
        "types/**/*"
    ],
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build": "tsc --build",
        "build:watch": "tsc --build --watch",
        "clean": "rimraf dist lib types *.tsbuildinfo",
        "integrity": "pnpm install && pnpm dedupe && pnpm install",
        "lint": "prettier --check \"*.js\" \"*.json\" \"src/**/*.ts\" \"tests/**/*.js\" \"style/**/*.less\" \"*.js\" \"*.json\"",
        "fix": "prettier --write \"*.js\" \"*.json\" \"src/**/*.ts\" \"tests/**/*.js\" \"style/**/*.less\" \"*.js\" \"*.json\"",
        "prepublishOnly": "pnpm run clean && pnpm run webpack:prod",
        "start": "http-server -p 4000 -o examples",
        "svgo": "svgo --config svgo.yaml",
        "svgo:uri": "svgo --config svgo.yaml --datauri=base64 -o -",
        "test:unit": "jest",
        "test:e2e": "playwright test",
        "test_ui": "playwright test --ui",
        "test": "npm-run-all test:*",
        "watch": "npm-run-all -p *:watch",
        "webpack": "webpack --color",
        "webpack:prod": "NODE_ENV=production webpack --color",
        "webpack:watch": "webpack --color --watch"
    },
    "dependencies": {
        "regular-table": "^0.6.9",
        "rxjs": "^7.8.1"
    },
    "devDependencies": {
        "@playwright/test": "^1.57.0",
        "@types/jest": "^30.0.0",
        "@types/react": "^19.2.7",
        "css-loader": "^7.1.2",
        "css-minimizer-webpack-plugin": "^7.0.2",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^5.6.5",
        "http-server": "^14.1.1",
        "jest": "^30.2.0",
        "jest-environment-jsdom": "^30.2.0",
        "jest-junit": "^16.0.0",
        "less": "^4.4.2",
        "less-loader": "~12.3.0",
        "mini-css-extract-plugin": "^2.9.4",
        "npm-run-all": "^4.1.5",
        "playwright": "^1.57.0",
        "postcss-loader": "^8.2.0",
        "prettier": "^3.6.2",
        "rimraf": "^6.1.2",
        "shx": "^0.4.0",
        "source-map-loader": "^5.0.0",
        "style-loader": "^4.0.0",
        "svg-url-loader": "^8.0.0",
        "svgo": "^4.0.0",
        "ts-jest": "^29.4.5",
        "ts-loader": "^9.5.4",
        "tsconfig-paths-webpack-plugin": "^4.2.0",
        "typescript": "^5.9.3",
        "webpack": "^5.103.0",
        "webpack-cli": "^6.0.1",
        "webpack-dev-server": "^5.2.2",
        "webpack-remove-empty-scripts": "^1.1.1"
    }
}
