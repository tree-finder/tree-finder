{
    "$schema": "http://json.schemastore.org/tsconfig",
    "compilerOptions": {
        "skipLibCheck": true,
        "lib": ["ES2019", "DOM"],
        "composite": true,
        "declaration": true,
        "declarationMap": true,
        "incremental": true,
        "module": "esnext",
        "moduleResolution": "node",
        "noEmitOnError": true,
        "noImplicitAny": true,
        "sourceMap": true,
        "strictNullChecks": true,
        "target": "es2019"
    }
}
